$("ul.errors").html("");
<% if @award.errors.any? %>
  <% @award.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"));
  <% end %>
<% else %>
  $(".awards-index").html("<%= escape_javascript(render 'index') %>");
  $("#awards-modal").modal("hide");
<% end %>
<p id="notice"><%= notice %></p>

<h1>Time Unities</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_unities.each do |time_unity| %>
      <tr>
        <td><%= time_unity.title %></td>
        <td><%= link_to 'Show', time_unity %></td>
        <td><%= link_to 'Edit', edit_time_unity_path(time_unity) %></td>
        <td><%= link_to 'Destroy', time_unity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Time Unity', new_time_unity_path %>
